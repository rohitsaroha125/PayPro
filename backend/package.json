{
   "name": ".",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "ts-node": "10.7.0",
      "@types/node": "^16.11.10",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "typeorm": "0.3.17",
      "reflect-metadata": "^0.1.13",
      "pg": "^8.4.0"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:run": "npx ts-node ./node_modules/.bin/typeorm migration:run -d ./src/data-source.ts",
      "migration:generate": "npx ts-node ./node_modules/.bin/typeorm migration:generate ./src/migrations/**/* -d ./src/data-source.ts"
   }
}